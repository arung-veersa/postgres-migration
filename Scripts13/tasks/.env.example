# Environment variables for Task 02 Conflict Updater
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# Snowflake (Analytics - Read Only)
SNOWFLAKE_ACCOUNT=
SNOWFLAKE_USER=
SNOWFLAKE_WAREHOUSE=
SNOWFLAKE_PRIVATE_KEY=

# Postgres (ConflictReport - Read/Write)
POSTGRES_HOST=
POSTGRES_USER=
POSTGRES_PASSWORD=

# Application
ENVIRONMENT=dev
LOG_LEVEL=INFO
# ACTION=                         # Empty = full pipeline (preflight,task01,task02,postflight); or comma-separated: task01_copy_to_staging,task02_00_conflict_update
